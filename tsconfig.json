{
  "compilerOptions": {
    "allowJs": true,
    "alwaysStrict": true,
    "checkJs": true,
    "declaration": true,
    "declarationMap": true,
    "jsx": "react",
    "jsxFactory": "jsx",
    "lib": ["es2015"],
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": true,
    "stripInternal": true,
    "target": "es2015"
  }
}
